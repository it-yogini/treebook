
<div class="page-header">
<h1>All Statuses</h1>
</div>
<div>
<iframe src='http://embed.verite.co/timeline/?source=0Agl_Dv6iEbDadHdKcHlHcTB5bzhvbF9iTWwyMmJHdkE&font=Bevan-PotanoSans&maptype=toner&lang=en&height=650' width='100%' height='650' frameborder='0'></iframe>
</div>

<% @statuses.each do |status| %>
  <div class="status">
    <strong><%= status.name %></strong>
    <p><%= status.content %></p>
    <div class="meta">
      <%= link_to time_ago_in_words(status.created_at) + " ago", status %>
      <span class="admin">
        | <%=link_to "Edit", edit_status_path(status) %> |
        <%=link_to "Delete", status, method: :delete, data: { confirm:"Are you sure you want to delete this entry?"} %>
      </span>
    </div>
  </div>
<% end %>
<div>
  <%= link_to "New Entry", new_status_path %>
</div>
